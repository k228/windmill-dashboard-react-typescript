{
  "name": "windmill-dashboard-react",
  "version": "0.4.0",
  "description": "A multi theme, completely accessible, with components and pages examples, (almost) ready for production dashboard.",
  "private": true,
  "scripts": {
    "cz": "git-cz",
    "release": "release-it",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test"
  },
  "dependencies": {
    "@craco/craco": "^6.2.0",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.5.0",
    "@testing-library/user-event": "7.2.1",
    "@windmill/react-ui": "github:k228/windmill-react-ui",
    "chart.js": "2.9.3",
    "classnames": "2.2.6",
    "faker": "4.1.0",
    "i18next": "^20.3.3",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-http-backend": "^1.2.6",
    "react": "^16.13.1",
    "react-chartjs-2": "2.9.0",
    "react-dom": "^16.13.1",
    "react-focus-lock": "2.4.0",
    "react-i18next": "^11.11.3",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "react-transition-group": "4.4.1",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "1.1.4",
    "@svgr/webpack": "5.4.0",
    "@types/autoprefixer": "10.2.0",
    "@types/chart.js": "2.9.3",
    "@types/classnames": "2.2.6",
    "@types/cssnano": "4.0.0",
    "@types/faker": "4.1.0",
    "@types/react": "^16.14.8",
    "@types/react-dom": "^17.0.6",
    "@types/react-router-dom": "5.1.7",
    "@types/react-transition-group": "4.4.1",
    "@types/testing-library__jest-dom": "5.9.5",
    "@types/testing-library__react": "10.2.0",
    "autoprefixer": "^9.8.6",
    "commitizen": "4.1.2",
    "cross-env": "7.0.2",
    "cssnano": "4.1.10",
    "cz-conventional-changelog": "3.2.0",
    "postcss": "^7.0.36",
    "postcss-cli": "7.1.1",
    "react-axe": "3.5.2",
    "release-it": "13.6.4",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.4",
    "tailwindcss-rtl": "^0.7.3"
  },
  "keywords": [
    "windmill",
    "dashboard",
    "admin",
    "tailwind",
    "react"
  ],
  "release-it": {
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
